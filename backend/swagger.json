          },
          {
            "name": "page",
            "in": "query",
            "description": "Page number",
            "type": "integer",
            "default": 1
          },
          {
            "name": "limit",
            "in": "query",
            "description": "Items per page",
            "type": "integer",
            "default": 20
          },
          {
            "name": "sortBy",
            "in": "query",
            "description": "Sort by field",
            "type": "string",
            "enum": [
              "endTime",
              "price",
              "newest",
              "bidCount"
            ],
            "default": "endTime"
          },
          {
            "name": "order",
            "in": "query",
            "description": "Sort order",
            "type": "string",
            "enum": [
              "asc",
              "desc"
            ],
            "default": "desc"
          },
          {
            "name": "highlightMinutes",
            "in": "query",
            "description": "Minutes to highlight new products",
            "type": "integer",
            "default": 30
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
